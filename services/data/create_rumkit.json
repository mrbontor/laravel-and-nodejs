{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$idcreate": "http://example.com/create.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
        "nama",
        "jenis",
        "kabupaten",
        "alamat",
		"keterangan"
    ],
    "properties": {
        "nama": {
            "$idcreate": "#/properties/nama",
            "type": "string",
            "title": "nama rumah sakit",
            "default": "",
			"minLength": 0,
            "maxLength": 40,
            "examples": [
                "Mr Bontor"
            ],
            "pattern": "^(.*)$"
        },
        "keterangan": {
            "$idcreate": "#/properties/keterangan",
            "type": "string",
            "title": "keterangan rumah sakit",
            "default": "",
            "minLength": 0,
            "maxLength": 100,
            "examples": [
                "saya adalah rumah sakit dunia"
            ],
            "pattern": "^(.*)$"
        }
    }
}
